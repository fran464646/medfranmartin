<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="regimen.Regimen" table="regimen">
        <id column="id" name="id" type="java.lang.Long" />
        <property column="nombre" name="nombre" type="java.lang.String" />
        <property column="condiciones" name="condiciones" type="java.lang.String" />
        <property column="idTipoHabitacion" name="idTipoHabitacion" type="java.lang.Long" />
    </class>
</hibernate-mapping>