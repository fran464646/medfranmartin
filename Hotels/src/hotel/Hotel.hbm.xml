<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hotel.Hotel" table="hotel">
        <id column="id" name="id" type="java.lang.Long" />
        <property column="nombre" name="nombre" type="java.lang.String" />
        <property column="ciudad" name="ciudad" type="java.lang.String" />
        <property column="calle" name="calle" type="java.lang.String" />
        <property column="descripcion" name="descripcion" type="java.lang.String" />
        <property column="categoria" name="categoria" type="java.lang.Integer" />
        <property column="telefono" name="telefono" type="java.lang.String" />
        <property column="correoElectronico" name="correoElectronico" type="java.lang.String" />
        <property column="enTemporada" name="enTemporada" type="java.lang.Boolean" />
    </class>
</hibernate-mapping>