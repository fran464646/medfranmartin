<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="reserva.Reserva" table="reserva">
        <id column="id" name="id" type="java.lang.Long" />
        <property column="idHabitacion" name="idHabitacion" type="java.lang.Long" />
        <property column="idTarifa" name="idTarifa" type="java.lang.Long" />
        <property column="nombre" name="nombre" type="java.lang.String" />
        <property column="apellido1" name="apellido1" type="java.lang.String" />
        <property column="apellido2" name="apellido2" type="java.lang.String" />
        <property column="email" name="email" type="java.lang.String" />
        <property column="telefono" name="telefono" type="java.lang.String" />
        <property column="numCuenta" name="numCuenta" type="java.lang.String" />
        <property column="observacion" name="observacion" type="java.lang.String" />
        <property column="fechaRealizacion" name="fechaRealizacion" type="java.util.Date" />
        <property column="fechaEntrada" name="fechaEntrada" type="java.util.Date" />
        <property column="fechaSalida" name="fechaSalida" type="java.util.Date" />
    </class>
</hibernate-mapping>