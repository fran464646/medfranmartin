<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="tarifa.Tarifa" table="tarifa">
        <id column="id" name="id" type="java.lang.Long" />
        <property column="idTipoHabitacion" name="idTipoHabitacion" type="java.lang.Long" />
        <property column="idRegimen" name="idRegimen" type="java.lang.Long" />
        <property column="nombre" name="nombre" type="java.lang.String" />
        <property column="descripcion" name="descripcion" type="java.lang.String" />
        <property column="precio" name="precio" type="java.lang.Double" />
    </class>
</hibernate-mapping>